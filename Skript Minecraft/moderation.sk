command /lookup [<offline player>]:
	trigger:
		if player has the permission "MODO" or "ADMIN":
			if arg 1 is set:
				if arg 1 has the permission "{@bypassperm}":
					send "{@prefixlook} &7Vous ne pouvez pas LooKup ce joueur !" to player
					send "{@prefixlook} &7%player% a essayer de regarder vous LooKup !" to arg 1
					stop
				else:
					send ""
					send "&aI--------I  LooKup de %arg 1%  I--------I"
					if {first.join.%arg 1%} is set:
						send "&aPremière Connexion : %{first.join.%arg 1%}%"
					else:
						send "&aPremière Connexion : Jamais"
					if {last.join.%arg 1%} is set:
						send "&aDernière Connexion : %{last.join.%arg 1%}%"
					else:
						send "&aDernière Connexion : Jamais"
					if {nombre.connect.%arg 1%} is set:
						send "&aNombre de Connexion : %{nombre.connect.%arg 1%}%"
					else:
						send "&aNombre de Connexion : 0"
					if {ip.%arg 1%} is set:
						send "&aIp : %{ip.%arg 1%}%"
					else:
						send "&aIp : Aucune IP n'a été enregistrée "
					if {nombre.mute.%arg 1%} is set:
						send "&aNombre de Mute : %{nombre.mute.%arg 1%}%"
					else:
						send "&aNombre de Mute : 0"
					if {nombre.ban.%arg 1%} is set:
						send "&aNombre de Ban : %{nombre.ban.%arg 1%}%"
					else:
						send "&aNombre de Ban : 0"
					if {nombre.tempban.%arg 1%} is set:
						send "&aNombre de TempBan : %{nombre.tempban.%arg 1%}%"
					else:
						send "&aNombre de TempBan : 0"
					if {nombre.freeze.%arg 1%} is set:
						send "&aNombre de Freeze : %{nombre.freeze.%arg 1%}%"
					else:
						send "&aNombre de Freeze : 0"
					if {nombre.avert.%arg 1%} is set:
						send "&aNombre d'avertissement : %{nombre.avert.%arg 1%}%"
					else:
						send "&aNombre d'avertissement : 0"
					if {nombre.avert.mute.%arg 1%} is set:
						send "&aNombre d'avertissement pour Mute : %{nombre.avert.mute.%arg 1%}%"
					else:
						send "&aNombre d'avertissement pour Mute : 0"
					if {nombre.avert.ban.%arg 1%} is set:
						send "&aNombre d'avertissement pour Ban : %{nombre.avert.ban.%arg 1%}%"
					else:
						send "&aNombre d'avertissement pour Ban : 0"
					if {nombre.kick.%arg 1%} is set:
						send "&aNombre de Kick : %{nombre.kick.%arg 1%}%"
					else:
						send "&aNombre de Kick : 0"
					if {nombre.alert.click.%arg 1%} is set:
						send "&aNombre d'alert CPS : %{nombre.alert.click.%arg 1%}%"
					else:
						send "&aNombre d'alert CPS : 0"
					send "&aNombre de mort : %{mort.%arg 1%}%"
					send "&aNombre de kill : %{kill.%arg 1%}%"
					send "&aRatio : %{ratio.%arg 1%}%"
					send "&aI--------I  LooKup de %arg 1%  I--------I"
					send ""
			else:
				send "{@prefixlook} &7De quelle joueur voulez-vous voir les informations ?"
		else:
			send "&cTu n'as pas le droit de voir les informations des joueurs ! &c&l[&cMODO&c&l]"

