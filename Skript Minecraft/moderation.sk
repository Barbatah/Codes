command /mute [<offline player>] [<text>] [<text>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is set:
				if arg 2 is set:
					if arg 3 is set:
						if arg 1 has the permission "{@bypassperm}":
							send "{@prefixmute} &7Vous ne pouvez pas mute ce joueur !" to player
							send "{@prefixmute} &7%player% a essayer de vous mute !" to arg 1
							stop
						else:
							set {mute.%arg 1%} to true
							set {mute.date.%arg 1%} to now
							set {mute.temps.%arg 1%} to arg 2
							set {mute.temps.fr.%arg 1%} to {mute.temps.%arg 1%}
							replace "s" with " seconde" in {mute.temps.fr.%arg 1%}
							replace "h" with " heure" in {mute.temps.fr.%arg 1%}
							replace "d" with " jour" in {mute.temps.fr.%arg 1%}
							replace "m" with " minute" in {mute.temps.fr.%arg 1%}
							set {mute.raison.%arg 1%} to arg 3
							set {mute.muteur.%arg 1%} to player
							send "{@prefixmute} &a%arg 1% a été mute !"
							add "&b%arg 1% &e➜ &b%{mute.temps.fr.%arg 1%}% &epour &b%arg 3% &epar &b%player%" to {mutelist::*}
							if {nombre.mute.%arg 1%} is not set:
								set {nombre.mute.%arg 1%} to 0
								add 1 to {nombre.mute.%arg 1%}
							else:
								add 1 to {nombre.mute.%arg 1%}
							if {msgadm.mute} is true:
								broadcast "{@msgadmmute}"
							wait {mute.temps.%arg 1%}
							remove "&b%arg 1% &e➜ &b%{mute.temps.fr.%arg 1%}% &epour &b%arg 3% &epar &b%player%" from {mutelist::*}
					else:
						send "{@prefixmute} &aPour quelle raison veux-tu le mute ?"
				else:
					send "{@prefixmute} &aPendant combien de temp ?"
			else:
				send "{@prefixmute} &aQuelle joueur veux-tu mute ?"
		else:
			send "&cTu n'as pas le droit de Mute &4[&cMODO&4]&c !"

