command /unfreeze [<player>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is set:
				if {freeze.%arg 1%} is true:
					set {freeze.%arg 1%} to false
					apply jump boost 250 to arg 1 for 1 second
					apply slowness 250 to arg 1 for 1 second
					apply blindness 250 to arg 1 for 1 second
					send "{@prefixfreeze} &bTu as unfreeze le joueur &b&l%arg 1%" to player
