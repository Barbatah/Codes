								else:
									if arg 1 has the permission "{@bypassperm}":
										send "{@prefixban} &7Vous ne pouvez pas bannir ce joueur !" to player
										send "{@prefixban} &7%player% a essayer de vous bannir !" to arg 1
										stop
									else:
										set {tempban.%arg 1%} to true
										set {tempban.temps.%arg 1%} to arg 2
										set {tempban.temps.fr.%arg 1%} to {tempban.temps.%arg 1%}
										replace "s" with " seconde" in {tempban.temps.fr.%arg 1%}
										replace "h" with " heure" in {tempban.temps.fr.%arg 1%}
										replace "d" with " jour" in {tempban.temps.fr.%arg 1%}
										replace "m" with " minute" in {tempban.temps.fr.%arg 1%}
										set {ban.counter.%arg 1%} to 0
										set {tempban.bannisseur.%arg 1%} to player
										set {tempban.date.%arg 1%} to now
										set {tempban.raison.%arg 1%} to arg 3
										set {bantotal.%arg 1%} to true
										send "{@prefixban} &aVous avez banni %arg 1% pendant %{tempban.temps.fr.%arg 1%}% pour %arg 3%"
										add "&b%arg 1% &e➜ &b%arg 3% &epar &b%player% &ependant &b%{tempban.temps.fr.%arg 1%}% &ele &b%{tempban.date.%arg 1%}%" to {tempbanlist::*}
										if {msgadm.tempban} is true:
											broadcast "{@msgadmtempban} "
											if arg 1 is online:
												kick arg 1 due to "{@msgtempban1}"
											if {nombre.tempban.%arg 1%} is not set:
												set {nombre.tempban.%arg 1%} to 0
												add 1 to {nombre.tempban.%arg 1%}
											else:
												add 1 to {nombre.tempban.%arg 1%}
											wait {tempban.temps.%arg 1%}
											remove "&b%arg 1% &e➜ &b%arg 3% &epar &b%player% &ependant &b%{tempban.temps.%arg 1%}% &ele &b%{tempban.date.%arg 1%}%" from {banlist::*}
		else:
			send "&cTu n'as pas le droit de tempban &4[&cMODO&4]&c !"

