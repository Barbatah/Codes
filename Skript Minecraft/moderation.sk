command /settime [<text>] [<text>]:
	permission: ADMIN
	permission message: &cTu n'as pas la permission de set les temps de tempban &c&l[&cADMIN&c&l] &c!
	trigger:
		if arg 1 is "aimbot" or "antikb" or "autoclick" or "bowaimbot" or "speedhack" or "fly" or "forcefield" or "killaura" or "nofall" or "nuker" or "fastbow" or "vanillatp" or "xray":
			if CheckTime(arg 2) is true:
				set {time.%arg 1%} to arg 2
				set {time.fr.%arg 1%} to arg 2
				replace "s" with " seconde" in {time.fr.%arg 1%}
				replace "h" with " heure" in {time.fr.%arg 1%}
				replace "d" with " jour" in {time.fr.%arg 1%}
				replace "m" with " minute" in {time.fr.%arg 1%}
				send "{@prefix} &aVous avez set le temps du tempban de %arg 1% à %{time.fr.%arg 1%}%"
				stop
			else:
				send "{@prefix} &cVous devez préciser une durée."
				send "&cSecondes = s"
				send "&cMinutes = m"
				send "&cHeures = h"
				send "&cJours = d"
				stop
		if arg 1 is not "aimbot" or "antikb" or "autoclick" or "bowaimbot" or "speedhack" or "fly" or "forcefield" or "killaura" or "nofall" or "nuker" or "fastbow" or "vanillatp" or "xray":
			send "{@prefix} &aQuelle temps de ban veux-tu set ! / Aimbot / Antikb / Autoclick / Bowaimbot / Speedhack / Fly / ForceField / KillAura / NoFall / Nuker / FastBow / VanillaTP / Xray"

