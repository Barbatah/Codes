command /vanish [<offline player>]:
	trigger:
		if player has the permission "MODO" or "ADMIN":
			if arg 1 is set:
				if arg 1 is online:
					if {vanish.%arg 1%} is false:
						set {vanish.%arg 1%} to true
						hide arg 1 from all players
						send "{@prefix} &aMode Vanish Activer !" to arg 1
						send "{@prefix} &aVous avez activer le Mode Vanish de %arg 1% !"
						add "&a%arg 1% Mode Vanish Activer Par %player%" to {vanishlist::*}
						set {vanish.vanisheur.%arg 1%} to player
						stop
					if {vanish.%arg 1%} is true:
						set {vanish.%arg 1%} to false
						reveal arg 1 from all players
						set {vanish.%arg 1%} to false
						remove "&a%player% Mode Vanish Activer" from {vanishlist::*}
						remove "&a%player% Mode Vanish Activer Par %{vanish.vanisheur.%player%}%" from {vanishlist::*}
						send "{@prefix} &cMode Vanish Désactiver !" to arg 1
						send "{@prefix} &cVous avez désactiver le Mode Vanish de %arg 1% !" to player
						stop
			if arg 1 is not set:
				if {vanish.%player%} is false:
					hide player from all players
					send "{@prefix} &aMode Vanish Activer !"
					add "&a%player% Mode Vanish Activer" to {vanishlist::*}
					set {vanish.%player%} to true
					stop
				if {vanish.%player%} is true:
					reveal player from all players
					send "{@prefix} &cMode Vanish Désactiver !"
					remove "&a%arg 1% Mode Vanish Activer" from {vanishlist::*}
					remove "&a%arg 1% Mode Vanish Activer Par %{vanish.vanisheur.%arg 1%}%" from {vanishlist::*}
					set {vanish.%player%} to false
					stop
				else:
					send "{@prefix} &aLe joueur %arg 1% n'est pas connecté !"
		else:
			send "&cTu n'as pas le droit de te rendre invisible &c&l[&cMODO&c&l]&c !"

