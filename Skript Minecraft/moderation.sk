command /unban [<offline player>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is set:
				if {bantotal.%arg 1%} is true:
					if {ban.%arg 1%} is true:
						remove "&b%arg 1% &e➜ &b%{ban.raison.%arg 1%}% &epar &b%{ban.bannisseur.%arg 1%}% &ele &b%{ban.date.%arg 1%}%" from {banlist::*}
					if {tempban.%arg 1%} is true:
						remove "&b%arg 1% &e➜ &b%{tempban.raison.%arg 1%}% &epar &b%{tempban.bannisseur.%arg 1%}% &ependant &b%{tempban.temps.fr.%arg 1%}% &ele &b%{tempban.date.%arg 1%}%" from {tempbanlist::*}
					if {banip.%arg 1%} is true:
						remove "&b%arg 1% &e➜ &b%{banip.raison.%arg 1%}% &epar &b%{banip.bannisseur}% &ele &b%{banip.date.%arg 1%}%" from {baniplist::*}
					set {ban.%arg 1%} to false
					clear {ban.raison.%arg 1%}
					clear {ban.bannisseur.%arg 1%}
					clear {ban.date.%arg 1%}
					clear {tempban.date.%arg 1%}
					clear {tempban.temp.%arg 1%}
					clear {tempban.temps.%arg 1%}
					clear {tempban.baninisseur.%arg 1%}
					clear {tempban.raison.%arg 1%}
					clear {banip.raison.%arg 1%}
					clear {banip.bannisseur.%arg 1%}
					clear {banip.date.%arg 1%}
					set {ban.counter.%arg 1%} to 0
					set {bantotal.%arg 1%} to false
					set {tempban.%arg 1%} to false
					set {banip.%arg 1%} to false
					send "{@prefixban} &aVous avez débanni le joueur %arg 1% !"
