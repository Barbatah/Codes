command /god [<offline player>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is not set:
				if {god.%player%} is false:
					set {god.%player%} to true
					send "{@prefix} &aMode Invinsible Activer !" to player
					add "&a%player% Mode Invinsible Activer" to {godlist::*}
					heal the player
					set the food level of player to 10
					stop
				if {god.%player%} is true:
					set {god.%player%} to false
					send "{@prefix} &cMode Invinsible Désactiver !" to player
					remove "&a%player% Mode Invinsible Activer" from {godlist::*}
					stop
			else:
				if arg 1 is online:
					if {god.%arg 1%} is false:
						set {god.%arg 1%} to true
						send "{@prefix} &aVous avez activer le Mode Invinsible de %arg 1%" to player
						send "{@prefix} &aMode Invinsible Activer" to arg 1
						set {god.godeur.%arg 1%} to player
						add "&a%arg 1% Mode Invinsible Activer Par %player%" to {godlist::*}
						heal the arg 1
						set the food level of arg 1 to 10
						stop
					if {god.%arg 1%} is true:
						set {god.%arg 1%} to false
						send "{@prefix} &cVous avez désactiver le Mode Invinsible de %arg 1% !" to player
						send "{@prefix} &cMode Invinsible Désactiver !" to arg 1
						remove "&a%arg 1% Mode Invinsible Activer Par %{god.godeur.%arg 1%}%" from {godlist::*}
						stop
				else:
					send "{@prefix} &aLe joueur %arg 1% n'est pas connecté !"
