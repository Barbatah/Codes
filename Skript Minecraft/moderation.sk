#===================================#

command /mod [<offline player>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is not set:
				if {mod.%player%} is false:
					set {mod.%player%} to true
					hide player from all players
					send " "
					send "&aMode Modération Activer !"
					send " "
					set player's flight mode to true
					set {vanish.%player%} to true
					set {fly.%player%} to true
					set {_count} to 0
					loop 36 times:
						set {mod.inventaire.%player%.%{_count}%} to slot {_count} of player's inventory
						add 1 to {_count}
					set {mod::casque::%player%} to player's helmet
					set {mod::plastron::%player%} to player's chestplate
					set {mod::pantalon::%player%} to player's leggings
					set {mod::bottes::%player%} to player's boots
					clear player's inventory
					set player's gamemode to survival
					set slot 0 of player's inventory to iron hoe named "&aVanish"
					set slot 1 of player's inventory to feather named "&aFly"
					set slot 2 of player's inventory to bone named "&aKill"
					set slot 4 of player's inventory to blaze rod named "&aRandomTP"
					set slot 6 of player's inventory to stick of knockback 1 and unbreaking 10 named "&bKB I"
					set slot 7 of player's inventory to stick of knockback 2 and unbreaking 10 named "&bKB II"
					set slot 8 of player's inventory to stick of knockback 10 and unbreaking 10 named "&bKB X"
					add "&a%player% Mode Modération Activer" to {modlist::*}
					heal the player
					set the food level of player to 10
					stop
				if {mod.%player%} is true:
					set {mod.%player%} to false
					set player's flight mode to false
					send " "
					send "&cMode Modération Désactiver"
					send " "
					clear player's inventory
					reveal player from all players
					set {vanish.%player%} to false
					set {fly.%player%} to false
					set {_count} to 0
					loop 36 times:
						set slot {_count} of player to {mod.inventaire.%player%.%{_count}%}
						add 1 to {_count}
					set the helmet of player to {mod::casque::%player%}
					set the chestplate of player to {mod::plastron::%player%}
					set the leggings of player to {mod::pantalon::%player%}
					set the boots of player to {mod::bottes::%player%}
					remove "&a%player% Mode Modération Activer" from {modlist::*}
					stop
			if arg 1 is set:
				if arg 1 is online:
					if {mod.%arg 1%} is false:
						set {mod.%arg 1%} to true
						hide arg 1 from all players
						send " " to arg 1
						send "&aMode Modération Activer par %player% !" to arg 1
						send " " to arg 1
						set arg 1's flight mode to true
						set {vanish.%arg 1%} to true
						set {fly.%arg 1%} to true
						set {_count} to 0
						loop 36 times:
							set {mod.inventaire.%arg 1%.%{_count}%} to slot {_count} of arg 1's inventory
							add 1 to {_count}
						set {mod::casque::%arg 1%} to arg 1's helmet
						set {mod::plastron::%arg 1%} to arg 1's chestplate
						set {mod::pantalon::%arg 1%} to arg 1's leggings
						set {mod::bottes::%arg 1%} to arg 1's boots
						clear arg 1's inventory
						set arg 1's gamemode to survival
						set slot 0 of arg 1's inventory to iron hoe named "&aVanish"
						set slot 1 of arg 1's inventory to feather named "&aFly"
						set slot 2 of arg 1's inventory to bone named "&aKill"
						set slot 4 of arg 1's inventory to blaze rod named "&aRandomTP"
						set slot 6 of arg 1's inventory to stick of knockback 1 and unbreaking 10 named "&bKB I"
						set slot 7 of arg 1's inventory to stick of knockback 2 and unbreaking 10 named "&bKB II"
						set slot 8 of arg 1's inventory to stick of knockback 10 and unbreaking 10 named "&bKB X"
						add "&a%arg 1% Mode Modération Activer Par %player%" to {modlist::*}
						heal the arg 1
						set the food level of arg 1 to 10
						set {mod.modeur.%arg 1%} to player
						stop
					if {mod.%arg 1%} is true:
						set {mod.%arg 1%} to false
						set player's flight mode to false
						send " " to arg 1
						send "&cMode Modération Désactiver Par %player% !" to arg 1
						send " " to arg 1
						clear arg 1's inventory
						reveal arg 1 from all players
						set {vanish.%arg 1%} to false
						set {fly.%arg 1%} to false
						set {_count} to 0
						remove "&a%arg 1% Mode Modération Activer Par %{mod.modeur.%arg 1%}%" from {modlist::*}
						loop 36 times:
							set slot {_count} of arg 1 to {mod.inventaire.%arg 1%.%{_count}%}
							add 1 to {_count}
						set the helmet of arg 1 to {mod::casque::%arg 1%}
						set the chestplate of arg 1 to {mod::plastron::%arg 1%}
						set the leggings of arg 1 to {mod::pantalon::%arg 1%}
						set the boots of arg 1 to {mod::bottes::%arg 1%}
						stop
		else:
			send "&cTu n'as pas le droit d'utiliser le /mod &c&l[&cMODO&c&l]&c !"

