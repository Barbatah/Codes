command /cps [<offline player>] [<text>]:
	trigger:
		if player has the permission "MODO" or "ADMIN":
			if arg 1 is set:
				if arg 2 is set:
					if arg 2 is "info":
						send ""
						send "&aI--------I  CPS de %arg 1%  I--------I"
						if {nombre.alert.click.%arg 1%} is set:
							send "&aNombre d'alert CPS : %{nombre.alert.click.%arg 1%}%"
						else:
							send "&aNombre d'alert CPS : 0"
						if {maxclick.%arg 1%} is set:
							send "&aCPS maximum atteint : %{maxclick.%arg 1%}%"
						else:
							send "&aCPS maximum atteint : 0"
						send "&aI--------I  CPS de %arg 1%  I--------I"
						send ""
					if arg 2 is "look" or "view":
						if arg 1 is online:
							set {clicks.gauche.%arg 1%} to 0
							set {clicks.droit.%arg 1%} to 0
							set {clickverif.%arg 1%} to true
							send "{@prefix} &aLancement Du Test, merci de patienter !"
							wait 1 seconds
							set {clickverif.%arg 1%} to false
							send "{@prefix} &aRésultat: %{clicks.gauche.%arg 1%}% CPS (gauche)"
							send "{@prefix} &aRésultat: %{clicks.droit.%arg 1%}% CPS (droit)"
							clear {clicks.gauche.%arg 1%}
							clear {clicks.droit.%arg 1%}
						else:
							send "{@prefix} &aLe joueur %arg 1% n'est pas connecté !"
					if arg 2 is not "look" or "view" or "info":
						send "{@prefix} &aUtilise / View \ pour voir les CPS en direct !"
						send "{@prefix} &aUtilise / Info \ pour voir les informations d'un joueur"
				else:
					send "{@prefix} &aUtilise / View \ pour voir les CPS en direct !"
					send "{@prefix} &aUtilise / Info \ pour voir les informations d'un joueur"
			else:
				send "{@prefix} &aQuelle joueur veux-tu vérifier !"
		else:
			send "&cTu n'as pas le droit de voir les CPS des joueur &c&l[&cMODO&c&l]&c !"

