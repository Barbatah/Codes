      #---- Partie Commandes ----#

#===================================#

#===================================#

      #---- Partie Ban ----#

#===================================#

command /ban [<offline player>] [<text>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is not set:
				send "{@prefixban} &7Séléctionne un Joueur !" to player
				stop
			else:
				if arg 2 is set:
					if arg 1 has the permission "{@bypassperm}":
						send "{@prefixban} &7Vous ne pouvez pas bannir ce joueur !" to player
						send "{@prefixban} &7%player% a essayer de vous bannir !" to arg 1
						stop
					else:
						if {ban.%arg 1%} is false:
							set {ban.bannisseur.%arg 1%} to "%player%"
							set {ban.%arg 1%} to true
							set {ban.raison.%arg 1%} to arg 2
							set {ban.date.%arg 1%} to now
