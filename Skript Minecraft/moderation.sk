command /fly [<offline player>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is not set:
				if {fly.%player%} is false:
					set {fly.%player%} to true
					set player's flight mode to true
					send "{@prefix} &aMode Fly Activer" to player
					add "&a%player% Mode Fly Activer" to {flylist::*}
					stop
				if {fly.%player%} is true:
					set {fly.%player%} to false
					set player's flight mode to false
					send "{@prefix} &cMode Fly Désactiver" to player
					remove "&a%player% Mode Fly Activer" from {flylist::*}
					stop
			if arg 1 is set:
				if arg 1 is online:
					if {fly.%arg 1%} is false:
						set {fly.%arg 1%} to true
						set arg 1's flight mode to true
						send "{@prefix} &aMode Fly Activer Par %player%" to arg 1
						send "{@prefix} &aMode Fly Activer Pour %arg 1%" to player
						add "&a%arg 1% Mode Fly Activer Par %player%" to {flylist::*}
						set {fly.flyeur.%arg 1%} to player
						stop
					if {fly.%arg 1%} is true:
						set {fly.%arg 1%} to false
						set arg 1's flight mode to false
						send "{@prefix} &cMode Fly Désactiver Par %player%" to arg 1
						send "{@prefix} &cMode Fly Désactiver Pour %arg 1%" to player
						remove "&a%arg 1% Mode Fly Activer Par %{fly.flyeur.%arg 1%}%" from {flylist::*}
						stop
				else:
					send "{@prefix} &aLe joueur %arg 1% n'est pas connecté !"
		else:
			send "&cTu n'as pas le droit de fly &c&l[&cMODO&c&l]&c !"
				
