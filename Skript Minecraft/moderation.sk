command /freeze [<offline player>] [<text>]:
	trigger:
		if player has the permission "MODO":
			if arg 1 is set:
				if arg 1 is online:
					if arg 2 is set:
						if arg 1 has the permission "{@bypassperm}":
							send "{@prefixfreeze} &7Vous ne pouvez pas freeze ce joueur !" to player
							send "{@prefixfreeze} &7%player% a essayer de vous freeze !" to arg 1
							stop
						else:
							if {freeze.%arg 1%} is false:
								set {freeze.%arg 1%} to true
								apply jump boost 250 to arg 1 for 999 days
								apply slowness 250 to arg 1 for 999 days
								apply blindness 250 to arg 1 for 999 days
								send "{@prefixfreeze} &bTu as &b&lFreeze &ble joueur &b%arg 1% !" to player
								send "{@prefixfreeze} &bTu as été &b&lFreeze &bpar &b%player% &bpour: &b%arg 2% &b!" to arg 1
								send "{@prefixfreeze} &bEn cas de déconnexion, tu seras sanctionné d'un &b&lban &b!" to arg 1
								set {freeze.freezer.%arg 1%} to player
								if {nombre.freeze.%arg 1%} is not set:
									set {nombre.freeze.%arg 1%} to 0
									add 1 to {nombre.freeze.%arg 1%}
								else:
									add 1 to {nombre.freeze.%arg 1%}
								if {msgadm.freeze} is true:
									broadcast "{@msgadmfreeze}"
								if arg 1 is online:
									loop 10 times:
										set action bar of arg 1 to "&bVous avez été Freeze par &b&l%player% &bpour &b&l%arg 2% !"
										wait 1 second
					else:
						send "{@prefixfreeze} &bPour quelle raison veux-tu freeze &b&l%arg 1% ?"
				else:
					send "{@prefixfreeze} &bLe joueur %arg 1% n'est pas connecté !"
			else:
				send "{@prefixfreeze} &bQuelle joueur veux-tu freeze ?"
		else:
			send "&cTu n'as pas le droit de freeze &4[&cMODO&4]&c !"

